{
    "name": "hyperloop2/lap-trinh-php",
    "description": "Segment Lap trinh php",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "illuminate/events": "^8.0",
        "illuminate/config": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/database": "^8.0",
        "hyperloop2/segment-commons": "dev-master"
    },
    "require-dev": {
        "symfony/dotenv": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Scaleflex\\LapTrinhPhp\\": "src/"
        },
        "classmap": ["src/Commands"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Scaleflex\\LapTrinhPhp\\TuanSegementServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "repo-name": {
            "type": "vcs",
            "url": "git@code.scaleflex.cloud:hyperloop2/segment-commons.git"
        }
    }
}
